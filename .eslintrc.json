{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
		
    "rules": {
			"linebreak-style": 0,
			"global-require": 0,
			"eslint linebreak-style": [0, "error", "windows"],
			"react/react-in-jsx-scope": "off",
			"react/prop-types": "off",
			"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
